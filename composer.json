{
    "name": "spyck/conversion-bundle",
    "description": "Conversion Bundle for Symfony",
    "license": "MIT",
    "authors": [
        {
            "name": "Willem Verspyck",
            "email": "willemverspyck@outlook.com",
            "homepage": "https://verspyck.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "database",
        "doctrine",
        "php",
        "conversion",
        "goal"
    ],
    "homepage": "https://github.com/willemverspyck/conversion-bundle",
    "type": "library",
    "require": {
        "php": "^8.4",
        "doctrine/collections": "^2",
        "doctrine/doctrine-bundle": "^3",
        "doctrine/orm": "^3",
        "doctrine/persistence": "^4",
        "symfony/config": "^7.4|^8",
        "symfony/console": "^7.4|^8",
        "symfony/dependency-injection": "^7.4|^8",
        "symfony/doctrine-bridge": "^7.4|^8",
        "symfony/framework-bundle": "^7.4|^8",
        "symfony/http-kernel": "^7.4|^8",
        "symfony/messenger": "^7.4|^8"
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "^3",
        "friendsofphp/php-cs-fixer": "^3",
        "phpunit/phpunit": "^12",
        "squizlabs/php_codesniffer": "^4"
    },
    "autoload": {
        "psr-4": {
            "Spyck\\ConversionBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spyck\\ConversionBundle\\Tests\\": "tests/"
        }
    }
}
